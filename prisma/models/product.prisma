model Product {

    id                          Int             @id @default(autoincrement())
    name                        String
    description                 String
    image                       String
    drawingModel                String
    unit                        String
    features                    String
    // Geral
    brand                       String?
    category                    String?
    subcategory                 String?
    unitMeasure                 String?
    sku                         String?
    validity                    String?
    liqWeight                   String?
    grossWeight                 String?
    mass                        String?
    volume                      String?
    // Nota Fiscal
    number                      String?
    productCode                 String?
    receiptDescription          String?
    cfop                        String?
    ncm                         String?
    commercialUnit              String?
    tributaryUnit               String?
    commercialQuantity          String?
    tributaryQuantity           String?
    commercialUnitValue         String?
    tributaryUnitValue          String?
    grossValue                  String?
    icmsOrigin                  String?
    additionalInfo              String?



}


