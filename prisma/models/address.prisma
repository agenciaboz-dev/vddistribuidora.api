model Address {
    id              Int             @id @default(autoincrement())
    cep             String
    street          String
    number          String
    district        String
    uf              String
    city            String

    personId        Int             
    person          Person          @relation(fields: [personId], references: [id], onDelete: Cascade)
}

model Person {
    id              Int             @id @default(autoincrement())
    addresses       Address[]
}

